            <string>/* Slow to a stop */
Deceleration = 500 / (room_speed * room_speed)
if speed &gt; Deceleration {
     speed = speed - Deceleration
} else {
    speed = 0
}

/* Push enemies */
EnemyHit = instance_place(x,y,oEnemy)
if instance_exists(EnemyHit) {
    PushFactor = 2
    EnemyHit.x = EnemyHit.x + lengthdir_x(speed * PushFactor,direction)
    EnemyHit.y = EnemyHit.y + lengthdir_y(speed * PushFactor,direction)
}

/* Destroy when we leave the room */
if x &lt; 0 or y &lt; 0 or x &gt; room_width or y &gt; room_height {
    instance_destroy()    
} else {
    //Explode when shot
    IncomingProjectile = instance_place(x,y,oProjectile)
    
    if instance_exists(IncomingProjectile) {
            instance_create(x,y,oLiquidExplosion)
            instance_destroy()
    }
}
</string>
