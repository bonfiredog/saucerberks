  <code>//Again, create the room, viewport, UI and Game Controller objects.
InitialiseRoom()
InitialisePlayableLevel()

//Set the background to be initially transparent.
background_alpha[0] = 0

//Create the background tints for the Tutorial Room (which are different
//from the usual tints, so need to be created here...)

NumberOfTints = 30

Colour1 = make_color_rgb(random(255),random(255), random(255))
Colour2 = make_color_rgb(random(255),random(255), random(255))

while instance_number(oTint) &lt; NumberOfTints {
NewTint = instance_create(random(room_width),random(room_height),oTint)
NewTint.image_blend = choose(Colour1,Colour2)
}

//Create the Tutorial Controller object. 
instance_create(room_width/2, room_height/2, oTutorial)

//If the rather long title sound from the Start room is still
//playing, stop it now, when the Tutorial starts, otherwise there
//may be a nasty overlap with the tutorial sound.
audio_stop_sound(aSaucerberks)</code>
