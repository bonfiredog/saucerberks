            <kind>1</kind>
            <string>/* This is the tutorial control, for some reason kept in the player's draw event. Don't ask me why.*/

//In the presence of the Tutorial control object, go ahead with all of this.
if instance_exists(oTutorial) and oTutorial.TutMode = "NoTitles" {

//Have all text follow the current viewport.
posx = x - (view_xview[0])
posy = y - (view_yview[0])

draw_set_font(fTutorial)
draw_set_colour(c_lime)
draw_set_halign(fa_center)
draw_set_valign(fa_center)

//CONTROLS SECTION

if Stage = "Controls" {

//Press W, and then delete the UI tip.
if WPressed = false and oMenu.Mode != "Titles" {
draw_set_colour(c_red)
draw_circle(posx,posy - 120,40,true)
draw_set_colour(c_lime)
draw_text(posx, posy - 120,"W")
if keyboard_check(ord('W')) {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
WPressed = true }
}


//Press S, and then delete the UI tip.
if SPressed = false and oMenu.Mode != "Titles"{
draw_set_colour(c_red)
draw_circle(posx,posy + 120,40,true)
draw_set_colour(c_lime)
draw_text(posx,posy + 120,"S")
if keyboard_check(ord('S')) {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SPressed = true }
}


//Press A, and then delete the UI tip.
if APressed = false and oMenu.Mode != "Titles"{
draw_set_colour(c_red)
draw_circle(posx - 200,posy,40,true)
draw_set_colour(c_lime)
draw_text(posx - 200,posy,"A")
if keyboard_check(ord('A')) {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
APressed = true }
}


//Press D, and then delete the UI tip.
if DPressed = false and oMenu.Mode != "Titles"{
draw_set_colour(c_red)
draw_circle(posx + 200,posy,40,true)
draw_set_colour(c_lime)
draw_text(posx + 200,posy,"D")
if keyboard_check(ord('D')) {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
DPressed = true }
}

//If all buttons pressed, left click to end the section.
if WPressed and SPressed and APressed and DPressed = true and LeftClicked = false and oMenu.Mode != "Titles" {
draw_set_colour(c_red)
draw_circle(posx + 300, posy + 120, 130, true)
draw_text(posx + 300,posy + 50,"Left Click")
draw_set_colour(c_lime)
draw_text(posx + 300,posy + 140,"Shoot a little yellow #ball. It's deadly!#Sometimes.")
if mouse_check_button(mb_left) {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
LeftClicked = true }
}


//Create a swarmer to shoot near the player, with its own little message (contained in its own Draw event).
if LeftClicked = true {
    Stage = "Swarmers"
    instance_create(x + 400, y - 200, oTutSpawn)
    TutSwarmer = instance_create(x + 400, y - 200, oSwarmer)
    }
}


//SWARMERS STAGE

if Stage ="Swarmers" and oMenu.Mode != "Titles" {

if SoundPlay1 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay1 = true
}

//BOOMERS STAGE

//If the swarmer is destroyed, create a Boomer near the player with its own DrawGUI event.
if instance_number(oSwarmer) &lt; 1 {
Stage = "Boomers"
instance_create(x - 400, y + 200, oTutSpawn)
TutorialBoomer = instance_create(x - 400, y + 200, oBoomer)
}
}

if Stage = "Boomers" and oMenu.Mode != "Titles" {

if instance_exists(TutorialBoomer) {
//Shoot it once.
if BoomerStage = "First" {
if oBoomer.BoomerShot = false and oMenu.Mode != "Titles" {

if SoundPlay2 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay2 = true
}

} else if oBoomer.BoomerShot = true and oMenu.Mode != "Titles" {
if SoundPlay3 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay3 = true
}

if BoomerStageChange &gt; 0 {
BoomerStageChange = BoomerStageChange - 1
} else if BoomerStageChange &lt;= 0 {
BoomerStage = "Second"
}
} 
} else if BoomerStage = "Second" {
//Laying bait for it.
if TutorialBoomer.ThingsEaten = 0 and oMenu.Mode != "Titles" {
if SoundPlay4 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay4 = true
}


} else if TutorialBoomer.ThingsEaten = 1 and oMenu.Mode != "Titles" {
//Finally destroying it.
if SoundPlay5 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay5 = true
}
}


//This controls the 'right-click' hint that disappears if the right mosue button is pressed.
if RightClicked = false and oMenu.Mode != "Titles"  {

draw_set_colour(c_red)
draw_circle(posx - 180, posy - 110, 90, true)
draw_text(posx - 180,posy - 130, "Right Click")
draw_set_colour(c_lime)
draw_text(posx - 180,posy - 100,"Shoot bait?")
if mouse_check_button(mb_right) {RightClicked = true }
}


//make sure there is always one Swarmer in the room for this section.
if instance_number(oSwarmer) &lt; 1 {
instance_create(x + 400, y - 500, oTutSpawn)
instance_create(x + 400, y - 500 , oSwarmer)
}


}


//Spawn some swarmers and boomers to practice, updating the text in line with the Countdown, which runs throughout this section.
//Once it has finished, everything has spawned, and the Player can do what they like until they decide to fly into the TutEnd object.
} else {
Countdown = Countdown - 0.09
BoomerStage = "Third"

if Countdown &gt; 100 {
if SoundPlay6 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay6 = true
}

draw_set_colour(c_red)
draw_circle(posx + 300, posy + 120, 150, true)
draw_set_colour(c_yellow)
draw_text(posx + 300, posy + 115, "That's it! You're#nearly ready to#tackle the debris fields...")
}

if Countdown &lt; 100 and Countdown &gt; 70 {
if SoundPlay7 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay7 = true
}

draw_set_colour(c_red)
draw_circle(posx + 300, posy + 120, 165, true)
draw_set_colour(c_lime)
draw_text(posx + 300, posy + 110, "Feel free to practice#until you are ready#to go. Just keep#something in mind...")
}

if Countdown &lt; 100 {
if instance_number(oBoomer) &lt; 1 {
instance_create(x + 400, y - 500, oTutSpawn)
instance_create(x + 400, y - 500, oBoomer)
}
if instance_number(oSwarmer) &lt; 3 {
XSpawn = random_range(200,500)
YSpawn = random_range(200,500)
instance_create(x - XSpawn, y - YSpawn, oTutSpawn)
instance_create(x - XSpawn, y - YSpawn, oSwarmer)
}
}

if Countdown &lt; 70 and Countdown &gt; 40 {
if SoundPlay8 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay8 = true
}

draw_set_colour(c_red)
draw_circle(posx + 300, posy + 120, 145, true)
draw_set_colour(c_lime)
draw_text(posx + 300,posy + 120,"You're going to be seeing#a lot of mess...")



//Create a load of randomly placed debris.
if TutDebrisSpawn = false {
if TutDebCount &gt; 0 {
ThisDebrisSpawnyNegative = random_range(-30,-800)
ThisDebrisSpawnyPositive = random_range(30,800)
ThisDebrisSpawnxNegative = random_range(-30,-800)
ThisDebrisSpawnxPositive = random_range(30,800)

ThisDebrisSpawnxChosen = choose(ThisDebrisSpawnxNegative,ThisDebrisSpawnxPositive)
ThisDebrisSpawnyChosen = choose(ThisDebrisSpawnyNegative,ThisDebrisSpawnyPositive)

instance_create(room_width/2 + ThisDebrisSpawnxChosen,room_height/2 + ThisDebrisSpawnyChosen,oTutSpawn)
ThisDebris = instance_create(room_width/2 + ThisDebrisSpawnxChosen,room_height/2 + ThisDebrisSpawnyChosen,choose(oDetritus1,oDetritus2,oDetritus3,oDetritus4,oDetritus5,oDetritus6,oDetritus7,oDetritus8,oDetritus9,oDetritus10,oDetritus11,oDetritus12,oDetritus13,oDetritus14,oDetritus15))
TutDebCount = TutDebCount - 1
} else if TutDebCount &lt;= 0 {
TutDebrisSpawn = true
}
}
}


//Create the TutEnd object, and update the text to encourage the player to fly into it.
if Countdown &lt; 60 and Countdown &gt; 40 {
if TutEndSpawn = false {
instance_create(oPlayer.x + 480, oPlayer.y + 200, oTutSpawn)
instance_create(oPlayer.x - 480, oPlayer.y + 200, oTutEnd)
TutEndSpawn = true }
}

if Countdown &lt; 35 and Countdown &gt; 0 {
if SoundPlay9 = false {
audio_sound_pitch(aTextChange,random_range(0.3,1.5))
audio_play_sound(aTextChange,1,false)
SoundPlay9 = true
}
if instance_exists(oTutEnd) and oTutEnd.PlayerTouch = false {
draw_set_colour(16711935)
draw_circle(posx + 300, posy + 120, 150, true)
draw_text(posx + 300,posy + 110,"Fly into that little#purple number when#you're ready to go!")

}
}

}
}
}

//This bit below is not to do with tutorial.

//When first picked up, display a message about the GrenadeGun.

if instance_exists(oGrenadeGun) and oGrenadeGun.PickUpMessage = true {

posx = x - view_xview[0]
posy = y - view_yview[0]

if TimeToDisplay &gt; 0 {
TimeToDisplay = TimeToDisplay - 1 
draw_set_font(fTutorial)
draw_set_colour(c_lime)
draw_text(posx,posy - 100,"A Reaper bomb module! Ooh! I saw#one on Grandma's wall once...")
} else if TimeToDisplay &lt;= 0 {
