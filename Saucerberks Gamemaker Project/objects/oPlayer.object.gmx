        <arguments>
          <argument>
            <kind>1</kind>
            <string>
//If there is no input and the player has moved (i.e. a speed of more than zero) and they aren't docked or undocking,
//drift in the current direction, slowing down all the time.
if keyboard_check(ord('W')) = false and keyboard_check(ord('S')) = false and keyboard_check(ord('A')) = false and keyboard_check(ord('D')) = false and speed &gt; 0 and Undocking = false and Docked = false {
DriftSpeed = 8 / room_speed
speed = speed - DriftSpeed
}

//Change the sprite to suit the gun being fired, giving the impression of lighting on the sprite.
if Docked = false and Undocking = false {
if instance_exists(oRapidFireGun) and mouse_check_button(mb_left) {
sprite_index = sPlayerFireGun
} else if instance_exists(oBaitGun) and mouse_check_button(mb_right) {
sprite_index = sPlayerFireBait
} else {
sprite_index = sPlayerJets
}
}


/* ------------------------------------------------------------------------------------------------------ */

//The following applies if the player is not docked (i.e. if they have control).
if Docked = false {
//Create a wake if the player is moving (once per step).
if speed &gt; 0 {
if room = rTutorial {
instance_create(x,y,oPlayerWake2)
} else {
instance_create(x,y,oPlayerWake)
}
}

//Put the player back into the room if they stray beyond it.
if x &lt; 0 {x = 0}
if y &lt; 0 {y = 0}

if x &gt; room_width {x = room_width}
if y &gt; room_height {y = room_height}


/* -------------------------------------------------------------------------------------------------------- */

//The following only occurs if the game is not paused and the player is not docked.
if Undocking = false and Docked = false {

/* Collision */

//If a block is about to touch us, it's big enough to collide with, and we aren't already colliding with something else...
SolidTouching = instance_place(x + hspeed, y + vspeed, oSolid)

if instance_exists(SolidTouching) and SolidTouching.image_xscale &gt; 0.8 and Colliding = false {
//Remember the player's speed just before the collision. If they were not moving, set the speed to the standard player speed.
OriginalSpeed = 350 / room_speed
ScreenShake(25)
//Play the collision sound.
if CollisionSoundPlayed = false {
audio_sound_pitch(aCollision,random_range(0.8,2))
audio_play_sound(aCollision,1,false)
CollisionSoundPlayed = true
}
//Create the spark.
instance_create(x + hspeed,y + vspeed,oContactFlash)
//Change the player's direction to almost the opposite of what they hit, and set them as 'in the process of colliding'.
if speed &gt; 0 {
direction = direction - random_range(130,220)
} else if speed &lt;= 0 {
direction = point_direction(x,y,SolidTouching.x,SolidTouching.y) - random_range(130,220)
}
//speed them up to simulate a 'bash' and 'shunt'
speed = OriginalSpeed + 300 / room_speed
Colliding = true
CollisionSoundPlayed = false
}


//If the player is 'in the process of colliding, set a deceleration rate, and have it decelerate by that amount.
if Colliding = true {
Deceleration = 0.5
if speed &gt; 0 {
speed = speed - Deceleration
} else if speed &lt;= 0 {
Colliding = false
}
}

}

/* ---------------------------------------------------------------------------------------------- */


//The following only occurs if the game is not paused, and the player is not 'in the process of colliding', and they are not docked.
if Colliding = false and Undocking = false and Docked = false{

/* Movement */
//Speed
MySpeed = 350 / room_speed

//Up
if keyboard_check(ord('W')) {
vspeed = -MySpeed
}


//Down
if keyboard_check(ord('S')) {
vspeed = MySpeed
}


//Left
if keyboard_check(ord('A')) {
hspeed = -MySpeed
}


//Right
if keyboard_check(ord('D')) {
hspeed = MySpeed
}



//If there is a Shunt happening, let it happen and then reset.
hspeed = hspeed + xShunt
vspeed = vspeed + yShunt
ResetShunts()

//face the direction of the mouse.
image_angle = point_direction(x,y,mouse_x,mouse_y)



/* ----------------------------------------------------------------------------------------------------------- */



/* Death */
//If an enemy class is touching the player...
EnemyTouchingMe = instance_place(x,y,oEnemy)
if instance_exists(EnemyTouchingMe) or (place_meeting(x,y,oGrenadeTargetExplosion) and oGrenadeTargetExplosion.Harmless = false) or place_meeting(x,y,oGrenadeBit) or place_meeting(x,y,oTurretDetTurretBolt) {
ScreenShake(80)
//Have the Swarmers all turn around and run away if they have destroyed the player.
with oSwarmer {
direction = point_direction(oPlayer.x,oPlayer.y,x,y)
RunAwaySpeed = true
RunAwayCountdown = random_range(120,240)
}
//...Explode into chunks, with how many bits the player is made of, using the player chunk object.
//Lower the live total.
global.PlayerLives = global.PlayerLives - 1
//Destroy the player's weapons.
with oBaitGun { instance_destroy() }
with oRapidFireGun { instance_destroy() }
if instance_exists(oGrenadeGun) and oGrenadeGun.Owner = oPlayer.id { with oGrenadeGun { instance_destroy() } } 
ExplodeIntoANumberOfChunks(oGame.NumberOfBitsThePlayerIsMadeOf, oBit)  
//Reset the tutorial if in the tutorial room.
if room = rTutorial {
instance_create(x,y,oTutorialDeath)
}
//Play the player death sound.
audio_play_sound(aPlayerDeath, 1, false) 
//Create an explosion object.
instance_create(x,y,oPlayerExplosion)
}
}

//If the player picks up the Grenade fun, flash green a certain number of times and beep.
if GrenadePickUpFlash = true {
if NumberOfFlashes &gt; 0 {
if TimeInBetweenFlashes &gt; 0 {
TimeInBetweenFlashes = TimeInBetweenFlashes - 1
} else if TimeInBetweenFlashes &lt;= 0 {
if TimeOfEachFlash &gt; 0 {
TimeOfEachFlash = TimeOfEachFlash - 1
image_blend = c_lime
} else if TimeOfEachFlash &lt;= 0 {
audio_sound_pitch(aFlashBeep, 1.8)
audio_play_sound(aFlashBeep,1,false)
image_blend = c_white
NumberOfFlashes = NumberOfFlashes - 1
TimeInBetweenFlashes = 15
TimeOfEachFlash = 6
}
}
} else if NumberOfFlashes &lt;= 0 {
GrenadePickUpFlash = false
}
}




//If we touch the Player Docking object, remove all functionality.

MyDock = instance_place(x,y,oPlayerDocking)
if instance_exists(MyDock) and Undocking = false and Docked = false {
Owner = MyDock
//Play the docking sound and create the Docking ring, with a randomised UI message.
audio_play_sound(aDock, 1, false)
instance_create(x,y,oDockingRing)
HidingDetDockedMessage = choose(
"Where are we? It smells in here...##Maybe this button does something!##I wonder what the U stands for...",
"An old Ark-class docking module!##What an antique!##'To eject, press the 'Smile' button'. Hmm...",
"I don't think they can see us anymore...##How do we get out of here?##There aren't any buttons!##Just this sign on the wall!##It says, 'What's the first letter##of the seventh planet?'" 
)

//Create a load of smoke, change the player sprite and the HidingDet sprite.
instance_create(x,y,oSmokeLauncher)
sprite_index = sPlayerDockedSprite
oHidingDet.sprite_index = sHidingDetDocked
Docked = true


}


/*All of this only applies if the player is currently docked with the HidingDet.*/

} else if Docked = true {

//Fade up the caption.
if CaptionAlpha &lt; 1 {
CaptionAlpha = CaptionAlpha + 0.005
}

//Fade out the player slightly.
if image_alpha &gt; 0.6 {
image_alpha = image_alpha - 0.006
}

//Follow the HidingDet, and hide the player's weapons (which woould show up because the player is slightly faded out.
x = Owner.x
y = Owner.y
vspeed = Owner.vspeed
hspeed = Owner.hspeed
direction = Owner.direction
image_angle = Owner.image_angle
oRapidFireGun.image_alpha = 0.0
oBaitGun.image_alpha = 0.0
if instance_exists(oGrenadeGun){
oGrenadeGun.depth = 82
oGrenadeGun.image_alpha = 0.0
}

//Change the depth to make sure the player displays underneath the Hiding Det.
depth = 81

//Flash red and beep.
if TimeBetweenFlashes &gt; 0 {
image_blend = c_white
TimeBetweenFlashes = TimeBetweenFlashes - 1
} else if TimeBetweenFlashes &lt;= 0 {
image_blend = c_red
TimeBetweenFlashes = 120
audio_play_sound(aBeep, 1, false)
}

//If the U button is pressed, fire the Player out, shrink and destroy the docking ring, reset
//all alphas, sprites and variables, and give the player a high speed and direction to fire out into the level.
if keyboard_check(ord('U')) {
Undocking = true
sprite_index = sPlayerJets
CaptionAlpha = 0.0
oDockingRing.Growing = false
oDockingRing.Shrinking = true
instance_create(x,y,oUndockingExplosion)
instance_create(x,y,oSmokeLauncher)
audio_play_sound(aUndock,1,false)
image_alpha = 1.0
oHidingDet.image_alpha = 1.0
oHidingDet.sprite_index = sHidingDet
image_blend = c_red
depth = -11
oRapidFireGun.image_alpha = 1.0
oBaitGun.image_alpha = 1.0
if instance_exists(oGrenadeGun){
oGrenadeGun.depth = -121
oGrenadeGun.image_alpha = 1.0 
}
speed = 1500 / room_speed
direction = Owner.image_angle
image_angle = random(360)
Owner = noone
Docked = false
}
}

//Deccelerate the player, and eventually stop the 'undocking' (i.e. missile) state, returning them to normal.
if Undocking = true {
speed = speed - 0.4
if speed &lt;= 0 {
image_blend = c_white
Undocking = false
}
}
