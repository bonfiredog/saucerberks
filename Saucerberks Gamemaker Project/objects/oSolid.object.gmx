            <string>
StayInsideRoom()

//Spin the solid.
image_angle = image_angle + SpinDirection

//Always slow down gently after any collision to the normal speed, both angularly and linearly.
if speed &gt; OriginalSpeed {
speed = speed - 0.25
}

/* Bouncing */


//If the Solid has not collided in the last two seconds,
//and collides with another solid or the player, bounce away,
//increasing the speed and the spin.

if MidstOfColliding &gt; 0 {
MidstOfColliding = MidstOfColliding - 1
} else if MidstOfColliding &lt;= 0 {
SetValues = false
}


SolidInContact = instance_place(x + hspeed,y+vspeed,oSolid) 
PlayerInContact = instance_place(x,y,oPlayer) 
ProjectileInContact = instance_place(x + hspeed, y + vspeed,oProjectile)

SolidCollision(SolidInContact)
SolidCollision(PlayerInContact)
SolidCollision(ProjectileInContact)

if SpinDirection &gt; OriginalSpinDirection {
SpinDirection = SpinDirection - 0.05
}

if SpinDirection &lt; OriginalSpinDirection {
SpinDirection = SpinDirection + 0.05
}


with oDetritus5 {
//If shot, explode (with sound) into a certain number of mini-asteroids, and then destroy itself.

if place_meeting(x,y,oProjectile){
instance_create(x,y,oAsteroidExplosion)
audio_play_sound(aRockBreak, 1, false)
ScreenShake(5)
image_alpha = 0.0
Hit = true
}

if Hit = true {
while MiniAsteroidsRemaining &gt; 0 {
MiniAsteroid = instance_create(x,y,oMiniAsteroid) 
MiniAsteroidsRemaining = MiniAsteroidsRemaining - 1
}
if MiniAsteroidsRemaining &lt;= 0{
instance_destroy()
}
}

}

</string>
